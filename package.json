{
  "name": "node-orm",
  "version": "1.0.0",
  "description": "sample node js project see README for details",
  "main": "src/functions/handler.ts",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "eslint . --ext .ts && jest --detectOpenHandles",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "clean": "git clean -fXd -e \\!data -e \\!data/**/* -e \\!node_modules -e \\!node_modules/**/*",
    "network": "docker network create shared"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tim-oe/node-101.git"
  },
  "keywords": [
    "aws",
    "lambda",
    "nodejs",
    "serverless",
    "localstack",
    "nestjs"
  ],
  "author": "tec",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/tim-oe/node-101/issues"
  },
  "homepage": "https://github.com/tim-oe/node-101#readme",
  "dependencies": {
    "@nestjs/common": "^7.6.15",
    "@nestjs/config": "^0.6.3",
    "@nestjs/core": "^7.6.15",
    "@nestjs/typeorm": "^7.1.5",
    "async": "^3.2.0",
    "aws-lambda": "^1.0.6",
    "aws-sdk": "^2.888.0",
    "cookie": "^0.4.1",
    "js-yaml": "^3.14.1",
    "mysql": "^2.18.1",
    "nest-winston": "^1.5.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.6.6",
    "typeorm": "0.2.28",
    "urlsafe-base64": "^1.0.0",
    "winston": "^3.3.3",
    "winston-cloudwatch": "^2.5.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.6.0",
    "@nestjs/schematics": "^7.3.0",
    "@nestjs/testing": "^7.6.15",
    "@types/async": "^3.2.6",
    "@types/aws-lambda": "^8.10.75",
    "@types/cookie": "^0.4.0",
    "@types/jest": "^26.0.22",
    "@types/js-yaml": "^4.0.1",
    "@types/node": "^14.14.36",
    "@types/supertest": "^2.0.10",
    "@types/urlsafe-base64": "^1.0.28",
    "@typescript-eslint/eslint-plugin": "^4.19.0",
    "@typescript-eslint/parser": "^4.19.0",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "jest": "^26.6.3",
    "jest-stare": "^2.2.2",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "serverless-localstack": "^0.4.30",
    "serverless-plugin-common-excludes": "^3.0.1",
    "serverless-plugin-include-dependencies": "^4.1.0",
    "serverless-plugin-typescript": "^1.1.9",
    "source-map-support": "^0.5.19",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.4",
    "ts-loader": "^8.0.18",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.2.3"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "/test/.*\\.spec\\.ts$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "collectCoverage": true,
    "coverageDirectory": "<rootDir>/reports/coverage",
    "testEnvironment": "node",
    "reporters": [
      "default",
      [
        "jest-stare",
        {
          "resultDir": "reports/tests",
          "coverageLink": "../coverage/lcov-report/index.html",
          "reportTitle": "test results",
          "reportHeadline": "test results"
        }
      ]
    ]
  }
}
